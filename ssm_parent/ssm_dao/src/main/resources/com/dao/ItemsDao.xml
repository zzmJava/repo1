<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.ItemsDao">
    <select id="findById" parameterType="java.lang.Integer" resultType="com.domain.Items">
        select * from items where id = #{id};
    </select>

</mapper>